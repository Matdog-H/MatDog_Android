<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.Share_files.List_share.Fragment_Shelter_Age">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/fa_recyclerview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <ImageView
        android:id="@+id/Img_nolist_AGE"
        android:layout_width="140dp"
        android:layout_height="110dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="180dp"
        android:src="@drawable/nolist_logo"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/Tv_nolist_AGE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="310dp"
        android:textSize="17dp"
        android:text="조건에 맞는 공고가 없다멍!"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/btn_newpost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/nolist_button_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="180dp"
        android:layout_marginLeft="50dp"
        android:background="#00000000"
        android:visibility="invisible"
        android:enabled="false"/>


    <ImageButton
        android:id="@+id/btn_rephoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/nolist_button_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="180dp"
        android:layout_marginRight="50dp"
        android:background="#00000000"
        android:visibility="invisible"
        android:enabled="false"/>

</androidx.constraintlayout.widget.ConstraintLayout>